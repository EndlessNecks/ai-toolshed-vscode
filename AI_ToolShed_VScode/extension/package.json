{
  "name": "ai-toolshed-vscode",
  "displayName": "AI ToolShed",
  "description": "Local RAG + Codestral integration for VS Code.",
  "version": "1.0.0",
  "publisher": "local",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onStartupFinished",
    "onCommand:ai-toolshed.restart",
    "workspaceContains:**"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ai-toolshed.restart",
        "title": "AI ToolShed: Restart RAG Server"
      }
    ]
  },
  "categories": ["Other"],
  "scripts": {
    "package": "vsce package"
  }
}
